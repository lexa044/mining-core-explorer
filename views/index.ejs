                      <tr>
                        <th scope="row">Miners</th>
                        <td>{{poolList.stats.connectedMiners}}</td>
                      </tr>
                      <tr>
                        <th scope="row">Hashrate</th>
                        <td>{{poolList.stats.hashrate}}</td>
                      </tr>
                      <tr>
                        <th scope="row">Network Difficulty</th>
                        <td>{{poolList.stats.networkDifficulty}}</td>
                      </tr>
                      <tr>
                        <th scope="row">Algorithm</th>
                        <td>{{poolList.stats.coinAlgorithm}}</td>
                      </tr>
                      <tr>
