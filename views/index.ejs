                        <td>
                            <a href="/faq#payments" target="_blank">
                                {{poolList.stats.payoutScheme}}
                            </a>                          
                        </td>
                      </tr>
                      <tr>
                        <th scope="row">Fee</th>
                        <td>{{poolList.stats.poolFeePercent}}%</td>
                      </tr>
                      <tr>
                        <th scope="row">Minimum Payout</th>
                        <td>{{poolList.stats.minimumPayment}} {{poolList.stats.coinSymbol}}</td>
                      </tr>
                      <tr>
                        <th scope="row">Miner Earnings</th>
                        <td>{{poolList.stats.totalPaid}} {{poolList.stats.coinSymbol}}</td>
                      </tr>
                      <tr>
                        <th scope="row">Pool Wallet</th>
                        <td>
                          <a href="{{poolList.stats.addressInfoLink}}" target="_blank">
                              {{poolList.stats.address}}
                          </a>
                        </td>
                      </tr>
                    </tbody>
                  </table>
              </div>
              <div class="tab-pane" id="tab_my_stats" role="tabpanel">
                  <table class="table table-sm" ng-if="poolList.minerInfo">
                      <tbody>
                        <tr>
                          <th scope="row">Hashrate</th>
                          <td>{{poolList.minerInfo.minerHashRate}}</td>
                        </tr>
                        <tr>
                          <th scope="row">Shares</th>
                          <td>{{poolList.minerInfo.pendingShares}}</td>
                        </tr>
                        <tr>
                          <th scope="row">Balance</th>
                          <td>{{poolList.minerInfo.pendingBalance}} {{poolList.stats.coinSymbol}}</td>
                        </tr>
                        <tr>
                            <th scope="row">Earned</th>
                            <td>{{poolList.minerInfo.paidBalance}} {{poolList.stats.coinSymbol}}</td>
                          </tr>
                          <tr>
                          <th scope="row">Last Payment</th>
                          <td>
                              <a href="{{poolList.minerInfo.lastPaymentLink}}" target="_blank">
                                  {{poolList.minerInfo.lastPayment}}
                              </a>                          
                          </td>
                        </tr>
                      </tbody>
                    </table> 
                    <h6 ng-if="poolList.minerInfo">Workers</h6>
                    <table class="table table-sm" ng-if="poolList.minerInfo">
                        <tbody>
                          <tr ng-repeat="worker in poolList.minerInfo.workers">
                            <td>{{::worker.label}}</td>
                            <td>{{::worker.hashrate}}</td>
                          </tr>
                        </tbody>
                      </table>                                 
                  <div class="input-group input-group-sm">
                    <input class="form-control form-control-sm" type="text" name="walletAddress" placeholder="Enter your wallet address" aria-label="Enter your wallet address" ng-model="walletAddress">
                    <div class="input-group-append">
                      <button class="btn btn-primary" type="submit" ng-click="poolList.getMinerInfo(walletAddress)">
                          <i class="fa fa-search"></i>
                      </button>
                    </div>
                  </div>
              </div>
            </div>        
