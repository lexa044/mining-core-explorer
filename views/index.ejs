              <thead>
                <tr>
                  <th scope="col">Miner</th>
                  <th scope="col">Hashrate</th>
                  <th scope="col">Sharerate</th>
                </tr>
              </thead>
              <tbody>
                <tr ng-repeat="miner in poolList.miners track by miner.miner">
                  <td>
                    <a href="{{::miner.walletInfoLink}}" target="_blank">
                      {{::miner.miner}}
                  </a>                    
                  </td>
                  <td>
                    {{::miner.hashrate}}                  
                  </td>
                  <td>
                    {{::miner.sharesPerSecond}}                  
                  </td>
                </tr>
              </tbody>
            </table>                    
